#!/bin/sh
#
# ~/.xinitrc
#
# Executed by startx (run your window manager from here)

if [ -d /etc/X11/xinit/xinitrc.d ]; then
  for f in /etc/X11/xinit/xinitrc.d/*; do
    [ -x "$f" ] && . "$f"
  done
  unset f
fi

if [ -f ~/.xprofile ]; then
	. ~/.xprofile
fi

{% if ("matexmonad" in desktop_wm_list) %}
exec mate-session
{% endif %}
exec xmonad
#exec gnome-session
